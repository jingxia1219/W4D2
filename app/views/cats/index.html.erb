<h1>These are our cats collection.</h1>
  
<% @cats.each do |cat| %> 
<ul>
  <li> <%= cat.name  %> </li>
  <ul>
    <li> <%= cat.birth_date  %> </li>
    <li> <%= cat.sex %> </li>
    <li> <%= cat.color%> </li>
    <li> <%= cat.description %> </li>
    <li> <a href="<%= cat_url(cat) %>">Cat profile</a></li>
  </ul>
</ul>
<% end %>